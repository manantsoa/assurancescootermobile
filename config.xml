<?xml version="1.0" encoding="utf-8" standalone="no"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="com.adobe.assurance" version="1.0.2">
    <name>AssuranceScooter</name>
    <description>
        An application which provide scooter insurance
    </description>
    <author email="contact@assuranceendirect.com" href="http://www.assurance-cyclo-scooter.com/">
        Assurance en Direct Team
    </author>
	<icon src="www/img/logoAssuranceScooterMobile.png"/>
	<splash src="www/img/yamaha-xmax-assurance-scooter-en-ligne.png"/>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist" version="1"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
</widget>
